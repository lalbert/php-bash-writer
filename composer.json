{
	"name" : "lalbert/bash-writer",
	"description" : "Write quickly and easily bash scripts with PHP",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Luc ALBERT",
			"homepage" : "https://lalbert.net/"
		}
	],
	"keywords" : [
		"bash"
	],
	"require" : {
		"symfony/console" : "~2.7 | ~3"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~5.3",
		"codacy/coverage" : "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"BashWriter\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"BashWriter\\Tests\\" : [
				"tests/unit"
			]
		}
	}
}
