{
	"name" : "lalbert/bash-writer",
	"description" : "Write quickly and easily bash scripts with PHP",
	"type" : "library",
	"license" : "MIT",
	"authors" : [{
			"name" : "Luc ALBERT",
			"homepage" : "https://lalbert.net/"
		}
	],
	"keywords" : [
		"bash",
		"shell",
		"php",
		"bash color",
		"bash style"
	],
	"require" : {
		"symfony/console" : "^2.5 || ^3.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.8 || ~5.3",
		"codacy/coverage" : "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"BashWriter\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"BashWriter\\Tests\\" : "tests/BashWriter/Test"
		}
	},
	"support" : {
		"issues" : "https://github.com/lalbert/php-bash-writer/issues",
		"source" : "https://github.com/lalbert/php-bash-writer"
	}
}
